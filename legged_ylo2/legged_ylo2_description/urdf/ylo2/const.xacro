    <xacro:property name="trunk_height" value="0.150"/>
    <xacro:property name="hip_radius" value="0.050"/>
    <xacro:property name="hip_length" value="0.06"/>

    <xacro:property name="thigh_shoulder_radius" value="0.044"/>
